{
  "name": "@kitiumai/env-type-generator",
  "version": "3.0.0",
  "description": "Zero-config TypeScript type generator for .env files with Zod validation, watch mode, and IDE autocomplete support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./generators": {
      "types": "./dist/generators/type-generator.d.ts",
      "import": "./dist/generators/type-generator.js",
      "require": "./dist/generators/type-generator.js"
    },
    "./parsers": {
      "types": "./dist/parsers/env-parser.d.ts",
      "import": "./dist/parsers/env-parser.js",
      "require": "./dist/parsers/env-parser.js"
    },
    "./services": {
      "types": "./dist/services/generator-service.d.ts",
      "import": "./dist/services/generator-service.js",
      "require": "./dist/services/generator-service.js"
    },
    "./utils": {
      "types": "./dist/utils/errors.d.ts",
      "import": "./dist/utils/errors.js",
      "require": "./dist/utils/errors.js"
    }
  },
  "bin": {
    "env-type-gen": "dist/cli.js"
  },
  "sideEffects": false,
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.js.map",
    "dist/**/*.d.ts.map",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*",
    "!dist/test/**",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepublishOnly": "npm run lint && npm run format:check && npm run build",
    "type-check": "tsc --noEmit",
    "typecheck": "tsc -b --noEmit",
    "format:fix": "prettier --write .",
    "changeset": "ensure-changeset && changeset",
    "version": "ensure-changeset && changeset version",
    "publish:set-token": "set-npm-token",
    "publish:setup": "set-npm-token",
    "publish:check": "npm whoami --registry https://registry.npmjs.org || npm login --registry https://registry.npmjs.org",
    "publish:login": "npm login --registry https://registry.npmjs.org",
    "publish:package": "pnpm run publish:check && pnpm publish --access public --no-git-checks --registry https://registry.npmjs.org",
    "publish:package:token": "pnpm run publish:setup && pnpm publish --access public --no-git-checks --registry https://registry.npmjs.org",
    "publish:package:otp": "pnpm run publish:check && pnpm publish --access public --no-git-checks --registry https://registry.npmjs.org --otp",
    "publish:dry-run": "pnpm publish --access public --no-git-checks --dry-run --registry https://registry.npmjs.org",
    "publish:dry-run:token": "pnpm run publish:setup && pnpm publish --access public --no-git-checks --dry-run --registry https://registry.npmjs.org",
    "release": "pnpm version && pnpm install --lockfile-only"
  },
  "keywords": [
    "typescript",
    "typescript-generator",
    "typescript-types",
    "type-generation",
    "type-generator",
    "type-safety",
    "type-safe",
    "strongly-typed",
    "env",
    "dotenv",
    "dotenv-typescript",
    "dotenv-types",
    "environment-variables",
    "environment",
    "env-variables",
    "env-types",
    "env-validation",
    "process-env",
    "process-environment",
    "code-generation",
    "codegen",
    "code-generator",
    "generator",
    "auto-generation",
    "validation",
    "schema-validation",
    "runtime-validation",
    "env-schema",
    "zod",
    "zod-schema",
    "zod-validation",
    "zod-typescript",
    "schema",
    "autocomplete",
    "intellisense",
    "ide-support",
    "vscode",
    "vscode-extension",
    "ide",
    "watch-mode",
    "file-watcher",
    "hot-reload",
    "cli",
    "cli-tool",
    "command-line",
    "npm-cli",
    "developer-tools",
    "devtools",
    "dev-tools",
    "dx",
    "developer-experience",
    "build-tools",
    "tooling",
    "nodejs",
    "node",
    "backend",
    "next",
    "nextjs",
    "next.js",
    "react",
    "vue",
    "nuxt",
    "express",
    "fastify",
    "nestjs",
    "nest",
    "koa",
    "hapi",
    "remix",
    "astro",
    "svelte",
    "sveltekit",
    "config",
    "configuration",
    "app-config",
    "env-file",
    "dotenv-safe",
    "dotenv-expand",
    "env-parser",
    "type-checking",
    "static-analysis",
    "compile-time",
    "zero-config",
    "zero-configuration",
    "productivity",
    "automation",
    "monorepo",
    "workspace",
    "pnpm",
    "npm",
    "yarn",
    "esm",
    "commonjs",
    "tree-shakeable",
    "lightweight",
    "type-declarations",
    "d.ts",
    "kitium",
    "kitiumai"
  ],
  "author": "KitiumAI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:kitium-ai/env-type-generator.git"
  },
  "bugs": {
    "url": "https://github.com/kitium-ai/env-type-generator/issues"
  },
  "homepage": "https://github.com/kitium-ai/env-type-generator#readme",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@kitiumai/config": "^2.0.0",
    "@kitiumai/lint": "^2.0.0",
    "@kitiumai/scripts": "^1.0.0",
    "@kitiumai/vitest-helpers": "^2.0.0",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "^5.6.0",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@kitiumai/logger": "^2.0.0",
    "@kitiumai/types": "^2.0.0",
    "@kitiumai/utils-ts": "^2.0.1",
    "chokidar": "^3.6.0",
    "commander": "^14.0.2",
    "dotenv": "^17.0.0",
    "zod": "^3.23.8"
  }
}
